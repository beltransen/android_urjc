<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="8dp"
    tools:context=".DetallesContactoActivity">

    <TextView
        android:id="@+id/detalleNombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/detalleTelefono"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tituloTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="4dp"
        android:text="@string/detalle_telefono"
        app:layout_constraintTop_toBottomOf="@+id/detalleNombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/detalleTelefono"
        app:layout_constraintBottom_toTopOf="@id/tituloEmail"/>
    <TextView
        android:id="@+id/detalleTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/tituloTelefono"
        app:layout_constraintStart_toEndOf="@id/tituloTelefono"/>

    <TextView
        android:id="@+id/tituloEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="4dp"
        android:text="@string/detalle_email"
        app:layout_constraintTop_toBottomOf="@+id/tituloTelefono"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/detalleEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/tituloEmail"
        app:layout_constraintStart_toEndOf="@id/tituloEmail" />

    <TextView
        android:id="@+id/tituloNacimiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="4dp"
        android:text="@string/detalle_nacimiento"
        app:layout_constraintTop_toBottomOf="@+id/tituloEmail"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/detalleNacimiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/tituloNacimiento"
        app:layout_constraintStart_toEndOf="@id/tituloNacimiento" />
</androidx.constraintlayout.widget.ConstraintLayout>